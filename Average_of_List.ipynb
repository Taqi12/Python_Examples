   "source": [
    "# define function\n",
    "def averageOfList(list):\n",
    "    return sum(list) / len(list)\n",
    "\n",
    "list = [1,2,3,4,5]\n",
    "print('The Average of the list is: ',averageOfList(list))"
   ]
