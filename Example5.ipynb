   "source": [
    "# create a list\n",
    "my_list = [\"20\", \"30\", \"40\", \"50\"]\n",
    "\n",
    "# initialize sum to zero\n",
    "sum = 0\n",
    "\n",
    "# apply for loop\n",
    "for i in my_list:\n",
    "    sum += int(i) # add element to sum variable after ever iteration\n",
    "    \n",
    "print(sum)"
   ]
